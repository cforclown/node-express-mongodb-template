NODE_ENV=production

// ============ PRODUCTION SERVER ============
SERVER_HOST=<production-host>
SERVER_PORT=<production-port>
APP_HOST=<production-app-host-port>

// ========== PRODUCTION AUTH SERVER =========
AUTH_SERVER_HOST=<production-auth-host>
AUTH_SERVER_PORT=<production-auth-port>
AUTH_APP_HOST=<production-app-host-port>

// =========== PRODUCTION DATABASE ===========
DB_HOST=<production-database-host>
DB_PORT=<production-database-port>
DB_USERNAME=<production-database-username>
DB_PASSWORD=<production-database-password>
DB_NAME=<production-database-name>

// ========= PRODUCTION SESSION CONFIG =======
SESSION_SECRET=<production-session-secret>
SESSION_RESAVE=false
SESSION_SAVE_UNINITIALIZED =false
SESSION_COOKIE_MAX_AGE=3600

// ========== PRODUCTION ACCESS TOKEN ========
ACCESS_TOKEN_SECRET=<access-token-secret>
REFRESH_TOKEN_SECRET=<refresh-token-secret>
ACCESS_TOKEN_EXP_IN=3600
